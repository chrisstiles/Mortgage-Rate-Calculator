{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "react-app",
    "eslint:recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "no-unused-expressions": "off",
    "babel/no-unused-expressions": "babel/no-unused-expressions"
  }
}